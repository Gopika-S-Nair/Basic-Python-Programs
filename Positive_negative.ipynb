      "source": [
        "# getting input from the user\n",
        "\n",
        "number = int(input())\n",
        "\n",
        "#condition\n",
        "\n",
        "if number == 0:\n",
        "  print('Zero')\n",
        "elif number > 0:\n",
        "  print('Positive')\n",
        "else:\n",
        "  print('Negative')"
      ]
    }
